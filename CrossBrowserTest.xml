<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="none" thread-count="2" name="DemoWebShop Functional Suite">
	<listeners>
		<listener class-name="commonLib.ExtentListener" />
	</listeners>
	<test name="EdgeTest">
		<parameter name="browser" value="edge" />
		<classes>
			<class name="tests.Login_Test" />
		</classes>
	</test>
	<test name="ChromeTest">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="tests.ItemPageTests" />
		</classes>
	</test>
</suite> 
